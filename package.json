{
  "name": "pensdk",
  "version": "0.0.12",
  "description": "Neosmartpen javascript library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "Test"
  ],
  "scripts": {
    "start": "node examples/server.js",
    "build:webpack": "rm -rf dist && webpack",
    "build": "rm -rf dist && tsc",
    "test": "jest",
    "clean": "rm -rf dist & rm -rf node_modules & rm package-lock.json & rm yarn.lock",
    "prepare": "yarn build",
    "deploy": "yarn publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neostudio-io/webPenSDK.git"
  },
  "keywords": [],
  "author": "AramMoon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neostudio-io/webPenSDK/issues"
  },
  "homepage": "https://github.com/neostudio-io/webPenSDK#readme",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "@types/jest": "^26.0.23",
    "babel-loader": "^8.2.2",
    "jest": "^27.0.4",
    "webpack": "^5.24.4"
  },
  "dependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "terser-webpack-plugin": "^5.1.1",
    "typescript": "^4.2.3",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3"
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ]
}
